    // Controle de Idade  
    document.getElementById('age-confirm').addEventListener('click', () => {  
      document.getElementById('ageCheck').style.display = 'none';  
      document.getElementById('mainContainer').style.display = 'block';  
      localStorage.setItem('ageVerified', 'true');  
    });  
    if (localStorage.getItem('ageVerified')) {  
      document.getElementById('ageCheck').style.display = 'none';  
      document.getElementById('mainContainer').style.display = 'block';  
    }  // Slideshow Autom√°tico  
let currentSlide = 0;  
const slides = document.querySelectorAll('.slide');  
function nextSlide() {  
  slides[currentSlide].classList.remove('active');  
  currentSlide = (currentSlide + 1) % slides.length;  
  slides[currentSlide].classList.add('active');  
}  
setInterval(nextSlide, 5000);  

// Controle de Modelos  
function showModel(name) {  
  const popup = document.getElementById('modelPopup');  
  // Carrega dados do modelo conforme o par√¢metro 'name'  
  document.getElementById('popupImage').src = 'https://i.ibb.co/N6fbYYTm/images-11.jpg';  
  document.getElementById('modelName').innerText = name.charAt(0).toUpperCase() + name.slice(1);  
  popup.style.display = 'block';  
}  

// Exemplo de Chatbot (fun√ß√£o para demonstra√ß√£o)  
function toggleChat() {  
  alert('Chatbot ativado!');  
}  

// Fun√ß√£o para exibir o popup de assinatura  
function showSubscriptionPopup() {  
  document.getElementById('subscriptionPopup').style.display = 'block';  
}  

// Fun√ß√£o para fechar um popup  
function closePopup(popup) {  
  popup.style.display = 'none';  
}  

// Fechar popups ao clicar fora do conte√∫do  
document.getElementById('modelPopup').addEventListener('click', function(e){  
  if(e.target === this) { closePopup(this); }  
});  
document.getElementById('subscriptionPopup').addEventListener('click', function(e){  
  if(e.target === this) { closePopup(this); }  
});  

// Efeitos Visuais para Modelos e Itens da Galeria  
document.querySelectorAll('.model, .gallery-item').forEach(item => {  
  item.addEventListener('mouseenter', () => {  
    item.style.transform = 'scale(1.02)';  
  });  
  item.addEventListener('mouseleave', () => {  
    item.style.transform = 'scale(1)';  
  });  
});  

// Timer de Oferta  
let offerTimer = 600; // 10 minutos  
setInterval(() => {  
  offerTimer--;  
  let minutes = Math.floor(offerTimer / 30);  
  let seconds = offerTimer % 30;  
  seconds = seconds < 10 ? '0' + seconds : seconds;  
  document.querySelector('.promo-banner').innerHTML =   
    `‚è≥ OFERTA REL√ÇMPAGO: ${minutes}:${seconds} - 30% OFF!`;  
  if(offerTimer <= 0) location.reload();  
}, 1000);

</script>  </body>

